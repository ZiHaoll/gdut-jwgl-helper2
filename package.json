{
  "name": "gdut-jwgl-helper2",
  "version": "0.3.5",
  "description": "基于 [vtmer/gdut-jwgl-helper](https://github.com/vtmer/gdut-jwgl-helper)，对新教务系统进行适配。",
  "main": "dist/helper.js",
  "browserslist": "chrome >= 60, firefox >= 58, safari >= 11, edge >= 14",
  "scripts": {
    "build:manifest": "node update-manifest.js",
    "build:js": "rollup -c",
    "build": "npm run build:js && npm run build:manifest",
    "lint": "eslint .",
    "format": "prettier --write \"src/**/*.js\" \"*.js\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dgeibi/gdut-jwgl-helper2.git"
  },
  "author": "dgeibi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dgeibi/gdut-jwgl-helper2/issues"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "eslint": "^5.5.0",
    "eslint-config-dgeibi": "^5.1.1",
    "prettier": "^1.0.0",
    "rollup": "^0.65.2",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-commonjs": "^9.1.6",
    "rollup-plugin-node-resolve": "^3.4.0"
  },
  "homepage": "https://github.com/dgeibi/gdut-jwgl-helper2#readme",
  "dependencies": {
    "file-saver": "^1.3.8"
  }
}
